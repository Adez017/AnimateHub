# URL of your backend API (used by the frontend to make API requests)
VITE_BACKEND_URL=http://localhost:5000

# Clerk Publishable Key for authentication (leave empty if not using Clerk yet)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_key_here

# GitHub Personal Access Token (used for GitHub API calls, e.g., fetching repositories)
VITE_GITHUB_TOKEN=your_github_token_here

# Vercel Analytics ID (used for tracking analytics in a Next.js/Vercel deployed app)
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_analytics_id
